{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "plugin:prettier/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "no-empty-function": "off",
    "@typescript-eslint/no-empty-function": ["error", {"allow": ["arrowFunctions"]}]
  }
}